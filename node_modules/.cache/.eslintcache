ainer } from '../../shopping-cart/components/cartContainer'\r\nimport { Link } from 'react-router-dom';\r\nimport '../style/store.css';\r\nimport { addCart, getCart } from '../../shopping-cart/dao/cartDao';\r\nimport { MembershipContainer } from '../../membership/components/membershipContainer';\r\n\r\nexport const Store = ({ page, notFound }) => {\r\n\r\n    const [pageState, setPageState] = useState(page);\r\n    const [cart, setCart] = useState([]);\r\n\r\n    const handleAddToCart = (product) => {\r\n        addCart(product.id).then(res => {\r\n            setCart(res);\r\n        })\r\n    }\r\n\r\n    useEffect(() => {\r\n        getCart().then(res => {\r\n            setCart(res);\r\n        })\r\n    }, [])\r\n\r\n    const handleCartIcon = () => {\r\n        setPageState(\"cart\");\r\n    }\r\n\r\n    const handleMembershipIcon = () => {\r\n        setPageState(\"membership\");\r\n    }\r\n\r\n    const handleHomeIcon = () => {\r\n        setPageState(\"home\");\r\n    }\r\n\r\n    const pageRenderSwitch = (pageState) => {\r\n        switch (pageState) {\r\n            case \"cart\":\r\n                return <CartContainer cart={cart} />\r\n            case \"membership\":\r\n                return <MembershipContainer></MembershipContainer>\r\n            default:\r\n                return <ProductContainer storeAddCart={handleAddToCart} />\r\n        }\r\n\r\n\r\n    }\r\n\r\n    return (\r\n        <>\r\n            <div title=\"Store\">\r\n                {/* Question 1. change the shop name */}\r\n                <div className=\"text-center mt-4\">\r\n                    <h1 className=\"display-3\">\r\n                        International Online Mall\r\n                    </h1>\r\n                    <p className=\"mt-3\">This is a online shopping website.</p>\r\n                </div>\r\n                <header className=\"header\">\r\n                    <Link className=\"header-child\" to=\"/\" onClick={handleHomeIcon}>Home</Link>\r\n                    <Link className=\"header-child\" to=\"/membership\" onClick={handleMembershipIcon}>Membership</Link>\r\n                    <Link className=\"header-child\">Sales</Link>\r\n                    <Link className=\"header-child\">Contact Us</Link>\r\n                    <Link className=\"header-child\" to=\"/cart\" onClick={handleCartIcon}><img className=\"header-icon\" src=\"/cart.png\" /> Cart ({cart ? cart.length : 0})</Link>\r\n                </header>\r\n            </div>\r\n            <div>\r\n                {\r\n                    notFound ? <div className=\"mt-5 text-center\">Seems you are lost, go back to <Link to=\"/\">home</Link></div> :\r\n\r\n                        (\r\n                            pageRenderSwitch(pageState)\r\n                        )\r\n                }\r\n\r\n\r\n\r\n            </div>\r\n        </>\r\n    )\r\n}\r\n","/Users/lkl/program/2021-r2-campus-recruitment/src/product/components/productContainer.js",[],"/Users/lkl/program/2021-r2-campus-recruitment/src/shopping-cart/components/cartContainer.js",["60"],"import { CartItem } from './cartItem';\r\nimport {deleteCart} from '../dao/cartDao'\r\nimport {useState} from 'react'\r\n\r\nexport const CartContainer = ({cart}) => {\r\n\r\n    const handleClear = () => {\r\n        deleteCart().then(()=>{\r\n            window.location.href = '/';\r\n        })\r\n    }\r\n\r\n    return (\r\n        <div>\r\n            { typeof cart === 'undefined' || cart.length === 0 ?\r\n                <div className=\"mt-5 text-center\">No item in cart</div>\r\n\r\n                :\r\n                <div className=\"row\">\r\n                    <div className=\"col-sm-3\"></div>\r\n                    <div className=\"card card-body border-0 col-sm-3\">\r\n                        {\r\n                            cart.map(product => <CartItem key={product.id} product={product} />)\r\n                        }\r\n                    </div>\r\n                    <div className=\"col-sm-1\"></div>\r\n                    <div className=\"col-sm-2\">\r\n                        <div className=\"card card-body\">\r\n                            <p className=\"mb-1\">Total Items</p>\r\n                            <h4 className=\" mb-3 txt-right\">{cart.length}</h4>\r\n                            <p className=\"mb-1\">Total Payment</p>\r\n                            <h3 className=\"m-0 txt-right\">{cart.map(e=>e.price).reduce((a, b) => a + b)}</h3>\r\n                            <hr className=\"my-4\" />\r\n                            <div className=\"text-center\">\r\n                                <button type=\"button\" className=\"btn btn-primary mb-2 btn-dark\" >CHECKOUT</button>\r\n                                <button type=\"button\" onClick={handleClear} className=\"btn btn-outlineprimary btn-sm\" >CLEAR</button>\r\n                            </div>\r\n\r\n                        </div>\r\n                    </div>\r\n\r\n                </div>\r\n            }\r\n\r\n        </div>\r\n\r\n    );\r\n}","/Users/lkl/program/2021-r2-campus-recruitment/src/shopping-cart/dao/cartDao.js",[],"/Users/lkl/program/2021-r2-campus-recruitment/src/membership/components/membershipContainer.js",[],"/Users/lkl/program/2021-r2-campus-recruitment/src/product/components/productItem.js",["61","62"],"import {useState} from 'react';\r\n\r\nexport const ProductItem = ({ id, name, description, price, onAddToCartClick }) => {\r\n\r\n  const [showAdded, setShowAdded] = useState(true);\r\n\r\n  const handleOnClick = () => {\r\n    onAddToCartClick({id:id, name:name,description:description,price:price});\r\n    setShowAdded(false);\r\n    setTimeout(function () {\r\n      setShowAdded(true);\r\n  }, 2000);\r\n  }\r\n\r\n  return (\r\n    <div key={id} className='card card-body'>\r\n      <img className=\"product-img\" src={\"/\" + id + \".png\"} />\r\n      <div className=\"mt-2\">{name}</div>\r\n      <p>${price}</p>\r\n      <p>\r\n        <div onClick={handleOnClick}><img className=\"add-to-cart-icon\" src=\"/cart.png\"/> <div hidden={showAdded} className=\"inline-block\">ADDED</div></div>\r\n      </p>\r\n    </div>\r\n  );\r\n}\r\n","/Users/lkl/program/2021-r2-campus-recruitment/src/shopping-cart/components/cartItem.js",["63"],"import '../style/cartStyle.css'\r\n\r\nexport const CartItem = ({ product }) => {\r\n\r\n  return (\r\n    <div key={product.id} className='row py-2'>\r\n      <div className=\"col-sm-2 p-2 mr-4\">\r\n        <img className=\"cart-product-img\" src={\"/\" + product.id + \".png\"} />\r\n      </div>\r\n      <div className=\"col-sm-5 p-2\">\r\n        <h4>{product.name}</h4>\r\n        <p>${product.price}</p>\r\n      </div>\r\n      <div className=\"col-sm-4 p-2 text-center \">\r\n        <p>X {product.quantity ? product.quantity : 1}</p>\r\n      </div>\r\n    </div>\r\n  );\r\n}","/Users/lkl/program/2021-r2-campus-recruitment/src/membership/components/membershipItem.js",["64"],"import {useState} from 'react';\r\nimport '../style/membershipStyle.css';\r\n\r\nexport const MembershipItem = ({ id, name, description, price }) => {\r\n\r\n\r\n  return (\r\n    <div key={id} className='card card-body square'>\r\n      <div className=\"mt-2 h4\">{name}</div>\r\n      <p>{description}</p>\r\n      <p className=\"price\">${price}</p>\r\n    </div>\r\n  );\r\n}\r\n","/Users/lkl/program/2021-r2-campus-recruitment/src/product/dao/productDao.js",[],"/Users/lkl/program/2021-r2-campus-recruitment/src/membership/dao/membershipDao.js",[],{"ruleId":"65","severity":1,"message":"66","line":66,"column":88,"nodeType":"67","endLine":66,"endColumn":135},{"ruleId":"68","severity":1,"message":"69","line":3,"column":9,"nodeType":"70","messageId":"71","endLine":3,"endColumn":17},{"ruleId":"65","severity":1,"message":"66","line":17,"column":7,"nodeType":"67","endLine":17,"endColumn":62},{"ruleId":"65","severity":1,"message":"66","line":21,"column":38,"nodeType":"67","endLine":21,"endColumn":89},{"ruleId":"65","severity":1,"messag